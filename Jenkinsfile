pipeline {
  agent any
  stages {
    stage('checkout') {
      steps {
        sh 'pwd'
        sh 'pwd'
        sh 'yarn install'
        sh 'pwd'
      }
    }

    stage('build') {
      steps {
        sh 'pwd'
        sh 'yarn build'
        sh 'pwd'
        sh 'pwd'
      }
    }

    stage('test') {
      steps {
        sh 'pwd'
        sh 'pwd'
        sh 'pwd'
        sh 'pwd'
        sh 'pwd'
        sh 'pwd'
      }
    }

  }
}
